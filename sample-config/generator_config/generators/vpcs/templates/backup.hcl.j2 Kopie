resource "ibm_is_vpc" "{{resourceName | replace("-", "_") }}" {
    name = "{{resourcePrefix}}{{resourceName}}"
    
    address_prefix_management = "{{ config.address_prefix_management | default('manual')}}"
    default_network_acl_name = "{{resourcePrefix}}{{resourceName}}-acl-default"
    default_security_group_name = "{{resourcePrefix}}{{resourceName}}-sg-default"
    default_routing_table_name "{{resourcePrefix}}{{resourceName}}-rt-default"
}

